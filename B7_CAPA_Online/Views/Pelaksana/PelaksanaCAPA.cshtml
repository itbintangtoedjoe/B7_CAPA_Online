                    checkTreatment = 0;
                    $('#Div_Treatment').attr("hidden", true);
                    checkCAPA();
                }
                $("#ddlKajianTreatment").empty();
                $("#ddlKajianTreatment").append(trhtml);
                $("#ddlKajianTreatment").select2();
