    function UploadPerbaikan()
    {

        var formData = new FormData();
        var idPerbaikan = $('#ddlPerbaikan').val();
        var kategori = "Perbaikan"
        formData.append("Kategori", kategori);
        formData.append("TindakanID", idPerbaikan);
        formData.append("Updater", nik);
        formData.append("NomorCAPA", noCAPA);
        var files = $('#uploadAttachmentPerbaikan').prop('files');
        for (var i = 0; i < files.length; i++) {
            var file = files[i];
            formData.append("Attachments", file);
        }

        if ($('#TxtLampiranCountPerbaikan').text() == "Tidak ada File Yang Dipilih") {
            $.unblockUI();
            return Swal.fire({
                title: "Error!",
                text: "Silahkan Pilih Attachment terlebih dahulu!",
                icon: "error",
                showConfirmButton: false,
                timer: 1000,
            });
        }
        $.ajax({
            url: "SimpanHasilPelaksanaan",
            type: "POST",
            data: formData,
            dataType: 'json',
            contentType: false,
            processData: false,
            success: function (data) {
                var result = JSON.parse(data);
                if (result[0].Validate == "Invalid") {

                    Swal.fire({
                        title: "Error!",
                        text: "File sudah ada!",
                        icon: "error",
                        showConfirmButton: false,
                        timer: 1000,
                    })
                }
                else {
                    Swal.fire({
                        title: "File Berhasil di Upload!",
                        icon: "success",
                        showConfirmButton: false,
                        timer: 1000,
                    })
                    $('#TxtLampiranCountPerbaikan').text("Tidak ada File Yang Dipilih");
                    GetPerbaikanAttachments();
                }
                $.unblockUI();
            },
            error: function (xhr, error, status) {
                console.log(error, status);
            }
        });

    }

