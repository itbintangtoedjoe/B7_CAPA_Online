                            title: "Error!",
                            text: "Silahkan pilih Evaluator Utama terlebih dahulu!",
                            icon: "error",
                            showConfirmButton: false,
                            timer: 1000,
                        })
                    }
                    else {

                        $.unblockUI();
                        Swal.fire({
                            title: "Success!",
                            text: "Submit Berhasil",
